
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title>NOUTSessionManager Bundle</title>
	{% block stylesheets %}
		<!-- icon -->
		<link rel="shortcut icon" href="{{ asset('bundles/noutsessionmanager/img/favicon.ico') }}" type="image/x-icon"/>
		<link rel="icon" href="{{ asset('bundles/noutsessionmanager/img/favicon.ico') }}" type="image/x-icon"/>

		<!-- feuille de style -->
		<link rel="stylesheet" href="{{ asset('vendors/bootstrap/css/bootstrap.min.css') }}" />
	{% endblock%}
</head>
<body>

<div class="container">
	<div id="loginbox" style="margin-top:50px;" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
		<div class="panel panel-info" >
			<div class="panel-heading">
				<div class="panel-title">{{ 'Sign In' | trans }}</div>
			</div>
			<div style="padding-top:30px" class="panel-body" >
				{{ 'No Answer from NOUTOnline' | trans }}
			</div>
			<div class="panel-footer">
				© NOUTWeb {{ noutwebsite_version }}
				{% if app.debug %}
				<br />
				© NOUTSessionManager Bundle
				{% endif %}
			</div>
		</div>
	</div>
</div>
</body>
</html>












