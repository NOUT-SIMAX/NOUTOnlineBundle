{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
	{# le contenu de la barre d'outils de débuggage web #}

	{# gestion de la couleur #}
	{% set color_code = collector.authenticated ? 'green' : 'red'  %}

	{% set icon %}
	{# l'icone de la plume pour SIMAX #}
	<img width="24" height="24" alt="NOUTOnline" src="data:image/png;base64," />
	{# s'il y a un utilisateur #}
	{% if collector.user %}
		<div class="sf-toolbar-status sf-toolbar-info-piece-additional sf-toolbar-status-{{ color_code }}">{{ collector.user }}</div>
		{% if collector.superviseur %}
			<img width="19" height="19" alt="superviseur" src="data:image/png;base64,"/>
		{% endif %}
	{% endif %}
	{# nombre de requetes #}
	<span class="sf-toolbar-status">{{ collector.querycount }}</span>
	{% if collector.querycount > 0 %}<div class="sf-toolbar-info-piece-additional-detail">in {{ '%0.2f'|format(collector.time * 1000) }} ms</div>{% endif %}
	{% endset %}

	{% set text %}
		{# informations sur la session #}
		{% if collector.authenticated %}
			<div class="sf-toolbar-info-piece">
				<b>Token session</b>
				<span class="sf-toolbar-status sf-toolbar-status-{{ color_code }}">{{ collector.sessiontoken }}</span>
			</div>
			<div class="sf-toolbar-info-piece">
				<b>Time zone</b>
				<span class="sf-toolbar-status sf-toolbar-status-{{ color_code }}">{{ collector.timezone }}</span>
			</div>
			<div class="sf-toolbar-info-piece">
				<b>Logged in as</b>
				<span class="sf-toolbar-status sf-toolbar-status-{{ color_code }}">{{ collector.user }}</span>
				{% if collector.superviseur %}
				<img width="19" height="19" alt="superviseur" src="data:image/png;base64,"/>
				{% endif %}
			</div>
		{% endif %}

		{# informations sur les requetes #}
		<div class="sf-toolbar-info-piece">
			<b>Queries</b>
			<span>{{ collector.querycount }}</span>
		</div>
		<div class="sf-toolbar-info-piece">
			<b>Query time</b>
			<span>{{ '%0.2f'|format(collector.time * 1000) }} ms</span>
		</div>
	{% endset %}

	{% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
<span class="label">
    <span class="icon"><img src="data:image/png;base64," alt="" /></span>
    <strong>NOUTOnline</strong>
    <span class="count">
        <span>{{ collector.querycount }}</span>
        <span>{{ '%0.0f'|format(collector.time * 1000) }} ms</span>
    </span>
</span>
{% endblock %}

{% block head %}
	{{ parent() }}

	<style>
		h3 {
			margin-bottom: 0px;
		}

		code {
			display: none;
			padding: 5px;
			background-color: #ffffff;
		}
	</style>

{% endblock %}



{% block panel %}
	{# le contenu du « panel » #}

	{# informations sur la session #}
	<h2>Session</h2>
	{% if collector.authenticated %}
		<table>
			<tr>
				<th>Username</th>
				<td>{{ collector.user }}</td>
			</tr>
			<tr>
				<th>Superviseur</th>
				<td>{% if collector.superviseur %} yes {% else %} no {% endif %}</td>
			</tr>
			<tr>
				<th>Token session</th>
				<td>{{ collector.sessiontoken }}</td>
			</tr>
			<tr>
				<th>Time zone</th>
				<td>{{ collector.timezone }}</td>
			</tr>

		</table>
	{% endif %}

	{# les requetes #}
	<h2>Queries</h2>

	{% if collector.queries is empty %}
		<p>
			<em>No queries.</em>
		</p>
	{% else %}
		<ul class="alt" id="queries">
		{% for i, query in collector.queries %}
			<li class="{{ cycle(['odd', 'even'], i) }}" data-extra-info="{{ '%0.2f'|format(query.executionMS * 1000) }}" data-target-id="{{ i }}">
				<div style="margin-top: 4px" id="queryNo-{{ i }}">
					{#
					<div>
						{{ query.operation | nl2br }} {{ '%0.2f'|format(query.executionMS * 1000) }} ms ({{ '%0.2f'|format(query.sendMS * 1000) }} ms)
					</div>
					#}
					<div>
						<div>
							<img title="Expand query" alt="+" id="expand-request-{{ i }}" class="expand" src="{{ asset('bundles/framework/images/blue_picto_more.gif') }}" style="display: inline; cursor: pointer;" />
							<img title="Hide query" alt="-" id="hide-request-{{ i }}" class="hide" src="{{ asset('bundles/framework/images/blue_picto_less.gif') }}" style="display:none ; cursor: pointer;" />
							<span>Request {{ query.operation | nl2br }} {{ '%0.2f'|format(query.executionMS * 1000) }} ms ({{ '%0.2f'|format(query.sendMS * 1000) }} ms)</span>
						</div>
						{% if query.soap and query.request_header is not empty%}
							<code class="code-request-{{ i }}">
								{{ query.request_header | noutonline_beautify_soap_query | nl2br }}
							</code>
							<code class="code-request-{{ i }} prettyprint">
								{{ query.request | noutonline_beautify_soap_query |nl2br }}
							</code>
						{% else %}
							<code class="code-request-{{ i }}">
								{{ query.request | noutonline_beautify_soap_query | nl2br }}
							</code>
						{% endif %}
						{#
						<code class="{% if query.soap %}prettyprint{% endif %}" id="code-request-{{ i }}">
							{{ query.request | noutonline_beautify_soap_query | nl2br }}
						</code>
						#}
					</div>
					<div>
						<div>
							<img title="Expand query" alt="+" id="expand-response-{{ i }}" class="expand" src="{{ asset('bundles/framework/images/blue_picto_more.gif') }}" style="display: inline; cursor: pointer;" />
							<img title="Hide query" alt="-" id="hide-response-{{ i }}" class="hide" src="{{ asset('bundles/framework/images/blue_picto_less.gif') }}" style="display: none; cursor: pointer;" />
							<span>Response</span>
						</div>

						{% if query.soap and query.response_header is not empty%}
							<code class="code-response-{{ i }}">
								{{ query.response_header | noutonline_beautify_soap_query | nl2br }}
							</code>
							<code class="code-response-{{ i }} prettyprint">
								{{ query.response | noutonline_beautify_soap_query |nl2br }}
							</code>
						{% else %}
							<code class="code-response-{{ i }}">
								{{ query.response | noutonline_beautify_soap_query | nl2br }}
							</code>
						{% endif %}
						{#
						<code class="{% if query.xml %}prettyprint{% endif %}" id="code-response-{{ i }}">
							{{ query.response | noutonline_beautify_soap_query |nl2br }}
						</code>
						#}
					</div>
				</div>
			</li>
		{% endfor %}
		</ul>

		<script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js?autoload=true"></script>


		{# <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script> #}
		<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

		<script type="text/javascript">//<![CDATA[

			$( document ).ready(function() {

				$('.expand').bind('click', function() {
					var expand_id = $(this).attr('id');
					var code_id = expand_id.replace("expand", "code");
					var hide_id = expand_id.replace("expand", "hide");

					$('.'+code_id).css('display', 'block');
					$('#'+hide_id).css('display', 'inline');
					$(this).css('display', 'none');
				});

				$('.hide').bind('click', function() {
					var hide_id = $(this).attr('id');
					var code_id = hide_id.replace("hide", "code");
					var expand_id = hide_id.replace("hide", "expand");

					$('.'+code_id).css('display', 'none');
					$('#'+expand_id).css('display', 'inline');
					$(this).css('display', 'none');
				});
			});
			//]]></script>


	{% endif %}
{% endblock %}